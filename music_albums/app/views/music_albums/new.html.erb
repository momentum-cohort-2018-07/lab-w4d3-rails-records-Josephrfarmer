<h1>New Album</h1>


<%= form_with scope: :album, local: true do |f| %>
<p>
  <%= f.label :title_name %><br/>
  <%= f.text_field :title_name %>
</p>
<p>
  <%= f.label :artist_name %><br/>
  <%= f.text_field :artist_name %>
</p>
<p>
   <%= f.label :year %><br/>
  <%= f.text_field :year %>
 </p>

  <%= f.submit %>

<% end %>